package core.commands;

import core.Command;

public class Configuration
	extends Command
{
	public Configuration()
	{
		super("config", "sets a program configuration setting");
	}

	@Override
	public void execute(String[] args) 
	{
		if (args == null)
		{
			printUsage();
			return;
		}
		
		if (args[0].equals("*"))
		{
			
		}
	}

	@Override
	public String help() 
	{
		return "config * or config <keyvalue name>";
	}
}
